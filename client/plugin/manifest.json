{
    "manifest_version": 2,
    "name": "RIL",
    "version": "0.1",

    "description": "Save links for later",

    "icons": {
        "24": "icons/add.svg"
    },
    "permissions": [
        "http://localhost:8000/*",
        "activeTab"
    ],
    "browser_action": {
        "title": "ril",
        "default_icon": {
            "24": "icons/add.svg"
        },
        "default_title": "RIL",
        "default_popup": "popup/ril.html"
    }
}
